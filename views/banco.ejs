<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <%- include('./partials/head', { title: 'Dados' }) %>
</head>
<body>
    
    <h1>DADOS DO BANCO DE DADOS</h1>
    <h2>Database: NODEDB</h2>
    <h3>Table: PESSOA</h3>

    <table class="table">
        <thead class="table-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Nome</th>
                <th scope="col">Telefone</th>
                <th scope="col">Endere√ßo</th>
                <th scope="col">Email</th>
                <th scope="col">CPF</th>
                <th scope="col">RG</th>
            </tr>
        </thead>
        <tbody>
            <% dados.forEach(dado => {%>
                <tr>
                    <th scope="row"><%= dado.ID %></th>
                    <td><%= dado.Nome %></td>
                    <td><%= dado.Telefone %></td>
                    <td><%= dado.Endereco %></td>
                    <td><%= dado.Email %></td>
                    <td><%= dado.CPF %></td>
                    <td><%= dado.RG %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>

    <%- include('./partials/footer') %>
    
</body>
</html>